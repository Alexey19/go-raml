#%RAML 1.0
title: Example API
version: v1
uses:
  files: libraries/files.raml
types:
  Place:
    properties:
      name: string
      dir: files.Directory

/dirs:
  get:
    responses:
      200:
        body:
          application/json:
            type:  files.Directory

/configs:
  type: files.file
  get:
    description: get config files
  post:
    responses:
      201:
        body:
          application/json:
            type: Place
